{
  "presets": [
    "es2015",
    "stage-0",
    "react"
  ],
  "plugins": [
    "universal-import",
    ["module-resolver", {
      "root": ["."],
      "alias": {
        "app": "./src"
      }
    }],
    "transform-decorators-legacy"
  ],
  "env": {
    "development": {
      "plugins": ["react-hot-loader/babel"]
    }
  } 
}
